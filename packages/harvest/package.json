{
  "name": "@rlbarn/harvest",
  "version": "1.0.0",
  "private": true,
  "main": "bin/index.js",
  "author": "Jeff Miller",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn build-core && yarn clean && yarn compile",
    "build-core": "cd ../core && yarn build-new",
    "clean": "npx rimraf -rf ./bin *.tsbuildinfo",
    "clean-logs": "npx rimraf -rf ./logs",
    "clean-images": "npx rimraf -rf ../api/public/item-images",
    "clean-env": "yarn clean-logs & yarn clean-images",
    "compile": "npx tsc -b",
    "env": "node -r dotenv/config",
    "scrape-products": "yarn env bin/scrapers/product/index.js"
  },
  "dependencies": {
    "@rlbarn/core": "1.0.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "jsdom": "^16.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  }
}
